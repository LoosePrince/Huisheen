{
  "name": "huisheen",
  "version": "1.0.0",
  "description": "回声 - 专注于通知接收的服务平台",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "setup": "node setup-env.js",
    "setup-env": "node setup-env.js",
    "docker:build": "docker build -t huisheen .",
    "docker:run": "docker run -d --name huisheen -p 3000:3000 huisheen",
    "docker:stop": "docker stop huisheen && docker rm huisheen",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f"
  },
  "keywords": ["notifications", "webhook", "api", "nodejs"],
  "author": "Huisheen Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "express-validator": "^7.0.1",
    "node-cron": "^3.0.3",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 